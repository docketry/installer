{
    "name": "docketry/installer",
    "description": "Docketry CMS installer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Develop Scotland",
            "email": "hello@develop.scot"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "bin": [
        "bin/docketry"
    ],
    "autoload": {
        "psr-4": {
            "Docketry\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docketry\\Installer\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
